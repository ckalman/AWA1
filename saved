            <script type="text/javascript">
                $(document).ready(function($) {

                    //Firefox Bug Fixing
                    var FF = !(window.mozInnerScreenX == null);
                    if(FF) {
                        // is firefox
                        $('.section7 .bg-transition').css('margin-top','1390px');

                    }



                    //Build Line
                    var line = new TimelineMax()
                            .add(TweenMax.to(".section7 .line1", 3.1, {height:"583px",delay: 0.2}))


                    // build scene
                    var scene = new ScrollScene({triggerElement: "#trigger9", reverse: false})
                            .setTween(line)
                            .addTo(controller);


                    // build Line and iPad pic---------------------------------
                    var line_tween = new TimelineMax()
                        //Show line and point
                            .add(TweenMax.to(".section7 .punkt1", 0.3, {height:"30px", width: "30px"}))
                            .add(TweenMax.to(".section7 .punkt2", 0.1, { css:{opacity:1}}))
                            .add(TweenMax.to(".section7 .punkt2", 0.5, {top:"300px"}))
                            .add(TweenMax.to(".section7 .tech1", 0.3, {width:"150px", height:"150px", top:"210px", ease: Linear.easeNone}))
                            .add(TweenMax.to(".section7 .punkt2", 0.5, {top:"510px"}))
                            .add(TweenMax.to(".section7 .tech2", 0.3, {width:"150px", height:"150px", top:"386px", ease: Linear.easeNone}))
                            .add(TweenMax.to(".section7 .punkt2", 0.5, {top:"625px"}))
                            .add(TweenMax.to(".section7 .tech3", 0.3, {width:"150px", height:"150px", top:"561px", ease: Linear.easeNone}))
                            .add(TweenMax.to(".section7 .punkt2", 0.5, {top:"763px"}))
                            .add(TweenMax.to(".section7 .punkt2", 0.1, {width:"0px", height:"0px"}))
                            .add(TweenMax.to(".section7 .punkt3", 0.1, {css:{opacity:1}}))
                            .add(TweenMax.to(".section7 .infotext4", 0.1, {css:{opacity:1}}));

                    // build scene
                    var scene = new ScrollScene({triggerElement: "#trigger9", reverse: false})
                            .setTween(line_tween)
                            .addTo(controller);

                    //Show/hide infotext-------------------
                    //bubbles fade in/out
                    function showbubbles(showhide, active) {

                        function fin() {
                            $('.sencha').fadeIn( 200, function() {
                                // Animation complete
                                $('.angular').fadeIn( 200, function() {
                                    // Animation complete
                                    $('.bootstrap').fadeIn( 200, function() {
                                        // Animation complete
                                    });
                                });
                            });
                        }
                        function fout() {
                            $('.bootstrap').fadeOut( 200, function() {
                                // Animation complete
                                $('.angular').fadeOut( 200, function() {
                                    // Animation complete
                                    $('.sencha').fadeOut( 200, function() {
                                        // Animation complete
                                    });
                                });
                            });
                        }
                        if(showhide=='show' && !$('.sencha').hasClass('isactive')) {
                            if(active=='active') {$('.sencha').addClass('isactive');}
                            fin();
                        }
                        else if(showhide=='show' && active=='active' && $('.sencha').hasClass('isactive')) {
                            if(active=='active') {$('.sencha').addClass('isactive');}
                            fout();
                            $('.sencha').removeClass('isactive');
                        }
                        else if(showhide=='hide' && active=='' && !$('.sencha').hasClass('isactive')) {
                            fout();
                        }
                    }
                    //Clickfunctions tech
                    $('.section7 .infotext').addClass('hover').click(function(){
                        if(!$(this).hasClass('infotext4')) {
                            $(this).addClass('hover').fadeOut();
                            $(this).addClass('active').fadeOut();
                        } else  if(!$(this).hasClass('minibubbletext')) {
                            $('.section7 div.minibubble.active').removeClass('active');
                        }
                    });

                    $('.section7 [class^=tech]').click(function() {
                        //read id
                        var id=$(this).attr("rel");
                        $('.section7 .infotext'+id).toggleClass('hover').show();

                        if(id==3) {
                            //animate bubbles of JS
                            showbubbles('show', 'active');
                        } else if (id>=8){
                            //Remove activ for devices section
                            $('.section7 .infotext'+id).removeClass('active').show();
                        }

                    }).hover(function() {
                        //read id
                        var id=$(this).attr("rel");
                        $('.section7 .infotext'+id+'.hover').fadeIn();
                        /*)
                        //show only, if nothing ist active by click
                        if(!$(".section7 div.active").length) {
                            $('.section7 .infotext'+id+'.hover').fadeIn();
                        }*/
                        if(id==3) {
                            showbubbles('show','');
                        }

                    });
                    //Clickfunctions bubbles JS
                    $('.section7 .minibubble').click(function() {
                        //read id
                        var id=$(this).attr("rel");
                        $('.section7 .minibubbletext').hide();
                        $('.section7 .infotext'+id).toggleClass('hover').show();

                        if($(".section7 div.minibubble.active").length==1) {
                            $('.section7 .minibubble'+id).removeClass('active');
                        } else {
                            $('.section7 div.minibubble.active').removeClass('active');
                            $('.section7 .minibubble'+id).addClass('active');
                        }

                    }).hover(function() {
                        //read id
                        var id=$(this).attr("rel");
                        //show only, if nothing ist active by click
                        if(!$(".section7 div.active").length) {
                            $('.section7 .infotext'+id+'.hover').fadeIn();
                        }
                    }, function() {
                        //read id
                        var id=$(this).attr("rel");
                        //show only, if nothing ist active by click
                        if(!$(".section7 div.active").length) {
                            $('.section7 .infotext'+id+'.hover').fadeOut();
                        }
                    });

                    //Cloud and box
                    var tween = new TimelineMax()
                        //Show line and point
                            .add(TweenMax.to(".section7 .line2", 0.4, {width:"156px"}))
                            .add(TweenMax.to(".section7 .line3", 0.5, {height:"202px"}))
                            .add(TweenMax.to(".section7 .box", 1, {css:{opacity:1}}))
                            .add(TweenMax.to(".section7 .line4", 0.5, {height:"400px"}))
                            .add(TweenMax.to(".section7 .cloud", 0.5, {css:{opacity:1}}))
                    // build scene
                    var scene = new ScrollScene({triggerElement: "#trigger10", reverse: false})
                            .setTween(tween)
                            .addTo(controller);

                    //Devices
                    TweenMax.set(".section7 .line6", {css:{opacity:1}}); // Opacity
                    TweenMax.set(".section7 .line6", {scaleX:0.001, transformOrigin:"0 50%"}); // scale line to 0
                    TweenMax.set(".section7 .line9", {css:{opacity:1}}); // Opacity
                    TweenMax.set(".section7 .line9", {scaleX:0.001, transformOrigin:"0 50%"}); // scale line to 0
                    var tween = new TimelineMax()
                        //Show line
                            .add(TweenMax.to(".section7 .line5", 0.2, {height:"65px", delay: 1.6}))
                            .add(TweenMax.to(".section7 .line6", 0.4, {scaleX:-1, ease: Linear.easeNone}))
                            .add(TweenMax.to(".section7 .line7", 0.2, {height:"70px"}))
                            .add(TweenMax.to(".section7 .devices", 2, {css:{opacity:1}, delay: 0.4}))
                            .add(TweenMax.to(".section7 .line8", 0.1, {height:"51px", delay: 0.4}))
                            .add(TweenMax.to(".section7 .line9", 0.4, {scaleX:-1, ease: Linear.easeNone}))
                            .add(TweenMax.to(".section7 .line10", 0.1, {height:"70px"}))
                            .add(TweenMax.to(".section7 .punkt4", 0.3, {height:"15px", width: "15px"}))

                    // build scene
                    var scene = new ScrollScene({triggerElement: "#trigger10", reverse: false})
                            .setTween(tween)
                            .addTo(controller);

                    //Show/hide cloud infotext-------------------
                    $('.section7 .device').click(function() {
                        //read id
                        var id=$(this).attr("rel");
                        $('.section7 .infotext'+id).toggleClass('hover').show();
                        $('.section7 .infotext'+id).toggleClass('active').show();

                    }).hover(function() {
                        //read id
                        var id=$(this).attr("rel");
                        //show only, if nothing ist active by click
                        if(!$(".section7 .devicetxt.active").length) {
                            $('.section7 .infotext'+id+'.hover').fadeIn();
                        }
                    });


                });
            </script>
        </section>

