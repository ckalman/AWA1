                    TweenMax.set(".section5 .linetext12", {rotation:-40}); //rotate

                    var line_tween = new TimelineMax()
                        //Show points and line
                            .add(TweenMax.to(".section5 .punkt1", 0.3, {height:"30px", width: "30px"}))
