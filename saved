                                TweenMax.to(".section3 .pyramid2-texta", 0.5, {css:{opacity:1}}),
                                TweenMax.to(".section3 .pyramid2-textb", 0.5, {css:{opacity:1}}),
                                TweenMax.to(".section3 .pyramid3-texta", 0.5, {css:{opacity:1}}),
                                TweenMax.to(".section3 .pyramid3-textb", 0.5, {css:{opacity:1}}),
