            <script type="text/javascript">
                $(document).ready(function($) {
                    //Counter

                    var number1_done = false;
                    var number2_done = false;
                    var number3_done = false;

                    function counterstart (name) {
                        if(window[name + '_done']) {
                            return;
                        }

                        $('.'+name).prop('Counter',0).animate({
                            Counter: $('.'+name).text()
                        }, {
                            duration: 2000,
                            easing: 'swing',
                            step: function (now) {
                                $('.'+name).text(Math.ceil(now));
                            }
                        });

                        window[name + '_done'] =true;

                    };

                    $('.number-button1').one('click mouseover', function() {
                        $('.number1b').css('display', 'none');
                        $('.number1').css('opacity', '1');
                        counterstart('number1')
                    });
                    $('.number-button2').one('click mouseover', function() {
                        $('.number2b').css('display', 'none');
                        $('.number2').css('opacity', '1');
                        counterstart('number2')
                    });
                    $('.number-button3').one('click mouseover', function() {
                        $('.number3b').css('display', 'none');
                        $('.number3').css('opacity', '1');
                        counterstart('number3')
                    });

                    //Source
                    $('.source-button').click(function () {
                        if(!$('.source-txt').hasClass('active')) {
                            $('.source-txt').animate({left: "529px"}, 'fast');
                            $('.source-txt').addClass('active');
                        } else {
                            $('.source-txt').animate({left: "-2000px"}, 'fast');
                            $('.source-txt').removeClass('active');
                        }
                    })

                    // build Line---------------------------------
                    TweenMax.set(".section3 .line4", {scaleX: 0.001, transformOrigin: "0 50%"}); // rotate scale line to 0
                    TweenMax.set(".section3 .line6", {scaleX: 0.001, transformOrigin: "0 50%"}); // rotate scale line to 0
                    TweenMax.set(".section3 .line10", {rotation:119, scaleX:0.001, transformOrigin:"0 50%"}); // rotate and scale line to 0
                    TweenMax.set(".section3 .line11", {rotation:22, scaleX:0.001, transformOrigin:"0 50%"}); // rotate and scale line to 0
                    TweenMax.set(".section3 .line12", {rotation:-23, scaleX:0.001, transformOrigin:"0 50%"}); // rotate and scale line to 0
                    TweenMax.set(".section3 .line13", {rotation:22, scaleX:0.001, transformOrigin:"0 50%"}); // rotate and scale line to 0
                    TweenMax.set(".section3 .line13", {scaleX:0.001, transformOrigin:"0 50%"}); // scale line to 0

                    var tween = new TimelineMax()

                        //Show line and first point
                            .add(TweenMax.to(".section3 .punkt1", 0.3, {height: "30px", width: "30px"}))
                            .add(TweenMax.to(".section3 .line1", 0.2, {height: "70px"}))
                            .add(TweenMax.to(".section3 .line2", 0.01, {css:{opacity:1}}))
                            .add(TweenMax.to(".section3 .line2", 0.7, {width: "450px", ease: Linear.easeNone}))
                            .add(TweenMax.to(".section3 .line3", 0.4, {height: "250px", ease: Linear.easeNone}))
                            .add(TweenMax.to(".section3 .line4", 0.8, {scaleX: -1,width: "800px", ease: Linear.easeNone}))
                            .add(TweenMax.to(".section3 .line5", 0.3, {height: "120px"}))
                            .add(TweenMax.to(".section3 .line9", 0.01, {css:{opacity:1}}))
                            .add(TweenMax.to(".section3 .line9", 0.8, {width: "800px", ease: Linear.easeNone}))
                            .add(TweenMax.to(".section3 .line8", 0.2, {height: "240px"}))
                            .add(TweenMax.to(".section3 .line6", 0.01, {css:{opacity:1}}))
                            .add(TweenMax.to(".section3 .line6", 0.4, {scaleX: -1, ease: Linear.easeNone}))
                            .add(TweenMax.to(".section3 .line7", 0.2, {height: "82px"}))
                            .add(TweenMax.to(".section3 .line10", 0.01, {css:{opacity:1}}))
                            .add(TweenMax.to(".section3 .line10", 0.2, {scaleX:1, ease: Linear.easeNone}))
                            .add(TweenMax.to(".section3 .line11", 0.01, {css:{opacity:1}}))
                            .add(TweenMax.to(".section3 .line11", 0.1, {scaleX:1, ease: Linear.easeNone}))
                            .add(TweenMax.to(".section3 .line12", 0.01, {css:{opacity:1}}))
                            .add(TweenMax.to(".section3 .line12", 0.1, {scaleX:1, ease: Linear.easeNone}))
                            .add(TweenMax.to(".section3 .line13", 0.01, {css:{opacity:1}}))
                            .add(TweenMax.to(".section3 .line13", 0.1, {scaleX:1, ease: Linear.easeNone}))
                            .add(TweenMax.to(".section3 .line14", 0.01, {css:{opacity:1}}))
                            .add(TweenMax.to(".section3 .line14", 0.1, {scaleX:1, ease: Linear.easeNone}))
