                            .add(TweenMax.to(".section3 .pyramid3", 1, {css:{opacity:1}}))
                            .add(TweenMax.to(".section3 .pyramid4", 1, {css:{opacity:1}}))
                            .add(TweenMax.to(".section3 .pyramid5", 1, {css:{opacity:1}}))
                            .add([
